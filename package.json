{
  "name": "gemini-code-api",
  "version": "2.0.0",
  "description": "高性能多协议AI网关服务，支持OpenAI、Claude与Gemini API的统一访问接口",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "deploy": "npm run build && wrangler deploy",
    "deploy:force": "npm run build && wrangler deploy --force",
    "logs": "wrangler tail",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "db:create": "wrangler d1 create gemini-code",
    "db:migrate": "wrangler d1 migrations apply gemini-code",
    "db:reset": "wrangler d1 execute gemini-code --command='DROP TABLE IF EXISTS request_logs; DROP TABLE IF EXISTS api_key_metrics; DROP TABLE IF EXISTS system_stats;' && npm run db:migrate"
  },
  "keywords": [
    "gemini",
    "openai",
    "claude",
    "api-gateway",
    "cloudflare-workers",
    "proxy",
    "load-balancer"
  ],
  "author": "Gemini Code API Team",
  "license": "MIT",
  "dependencies": {
    "hono": "^4.9.2",
    "zod": "^3.23.8",
    "drizzle-orm": "^0.33.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@cloudflare/workers-types": "^4.20241218.0",
    "drizzle-kit": "^0.24.0",
    "typescript": "^5.5.0",
    "vitest": "^2.0.0",
    "wrangler": "^4.31.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}